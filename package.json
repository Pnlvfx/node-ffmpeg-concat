{
  "name": "node-ffmpeg-concat",
  "description": "Node interaction with ffmpeg",
  "version": "2023.7.6",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "license": "MIT",
  "bin": {
    "ffmpeg-concat": "dist/esm/cli.js"
  },
  "scripts": {
    "dev": "tsc -w --outDir dist/esm & node --watch .",
    "start": "node .",
    "lint": "eslint ./src",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "commander": "^2.19.0",
    "ffmpeg-on-progress": "^1.0.0",
    "ffmpeg-probe": "^1.0.6",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.1",
    "get-pixels": "^3.3.3",
    "gl": "^6.0.1",
    "gl-buffer": "^2.1.2",
    "gl-texture2d": "^2.1.0",
    "gl-transition": "^1.13.0",
    "gl-transitions": "^1.43.0",
    "left-pad": "^1.3.0",
    "ndarray": "^1.0.19",
    "p-map": "^2.0.0",
    "rmfr": "^2.0.0",
    "sharp": "^0.31.1",
    "tempy": "^0.2.1",
    "url-parse": "^1.4.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/fs-extra": "^11.0.1",
    "@types/get-pixels": "^3.3.2",
    "@types/gl": "^6.0.2",
    "@types/gl-texture2d": "^2.1.4",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2"
  }
}
